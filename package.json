{
  "name": "@york.ie.parth.t/utils",
  "version": "1.0.4",
  "description": "Comprehensive utilities with full TypeScript support",
  "author": "Parth B Thakkar",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup src/index.ts --dts --format esm,cjs --out-dir dist --watch --clean",
    "build": "tsup src/index.ts --dts --format esm,cjs --out-dir dist --clean",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/parthbt-yorkie/york.ie.parth.t-utils.git"
  },
  "keywords": [
    "string",
    "case",
    "conversion",
    "typescript",
    "utils"
  ],
  "bugs": {
    "url": "https://github.com/parthbt-yorkie/york.ie.parth.t-utils/issues"
  },
  "homepage": "https://github.com/parthbt-yorkie/york.ie.parth.t-utils#readme",
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  }
}
