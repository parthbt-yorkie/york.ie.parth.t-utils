{
  "name": "@york.ie.parth.t/utils",
  "version": "1.0.2",
  "description": "Comprehensive utilities with full TypeScript support",
  "author": "Parth B Thakkar",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm --out-dir dist --clean && node scripts/postbuild.cjs",
    "prepare": "npm run build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/parthbt-yorkie/york.ie.parth.t-utils.git"
  },
  "keywords": [
    "string",
    "case",
    "conversion",
    "typescript",
    "utils"
  ],
  "bugs": {
    "url": "https://github.com/parthbt-yorkie/york.ie.parth.t-utils/issues"
  },
  "homepage": "https://github.com/parthbt-yorkie/york.ie.parth.t-utils#readme",
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "jest": "^29.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  }
}
